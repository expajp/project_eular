#include <stdio.h>
#include <string.h>
#define N 10

int main(int argc, char argv[]){
  int i, j, k, buffer;
  int result[3]={0, 0, 0};
  char before[N] = {'\0'};
  char after[N] = {'\0'};

  for(i=100;i<1000;i++){
    for(j=100;j<1000;j++)
      buffer = i*j;
      sprintf(before, "%d", buffer);
    
      for(k=strlen(before)-1;k>=0;k--){
	after[strlen(before)-k+1] = before[k];
      }
      if(strcmp(before, after) == 0){
	result[0] = i;
	result[1] = j;
	result[2] = buffer;
      }

    }
  }

  printf("%d Ã— %d = %d\n",result[0], result[1], result[2]);

  return 0;
}
